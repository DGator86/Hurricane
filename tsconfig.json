{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext",
      "WebWorker"
    ],
    "types": ["vite/client", "@cloudflare/workers-types"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "exclude": [
    "dist",
    "node_modules",
    "scripts",
    "public",
    "predictions",
    "predictions_enhanced",
    "predictions_yahoo"
  ]
}